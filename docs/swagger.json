        "/pub/stock/any_api": {
            "get": {
                "description": "当前节any-api",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "default"
                ],
                "summary": "当前节点any-api",
                "operationId": "NodeAnyApiHandler",
                "parameters": [
                    {
                        "type": "string",
                        "default": "https://min-api.cryptocompare.com/data/pricemultifull?fsyms=ETH\u0026tsyms=USD",
                        "description": "数据url",
                        "name": "req",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "default": "RAW.ETH.USD.VOLUME24HOUR",
                        "description": "指向数据字段的json path",
                        "name": "path",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "stock info",
                        "schema": {
                            "type": "string"
                        },
